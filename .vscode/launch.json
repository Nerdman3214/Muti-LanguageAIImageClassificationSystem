{
    "version": "0.2.0",
    "configurations": [
        

        {
            "name": "C++: Debug InferenceEngine",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/cpp/build/InferenceEngine",
            "args": [
                "${workspaceFolder}/models/resnet50_imagenet.onnx",
                "${workspaceFolder}/test_images/cat.jpg"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "Build C++ Project (CMake)",
            "miDebuggerPath": "/usr/bin/gdb"
        },
        {
            "name": "Python: Training",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/python/training/Training.py",
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "Python: Export ONNX",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/python/export/export_imagenet_resnet50_to_onnx.py",
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "type": "java",
            "name": "Java: AIController REST API",
            "request": "launch",
            "mainClass": "ai.controller.AIController",
            "projectName": "Muti-LanguageAIImageClassificationSystem_94de3473",
            "cwd": "${workspaceFolder}"
        }
    ]
}